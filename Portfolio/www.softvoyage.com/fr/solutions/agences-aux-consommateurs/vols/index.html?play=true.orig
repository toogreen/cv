        <!-- Scripts ajoutés par David pour remember cookies -->

        <script type="text/javascript">


            function setCookie(cname,cvalue,exdays)
            {
            var d = new Date();
            d.setTime(d.getTime()+(exdays*24*60*60*1000));
            var expires = "expires="+d.toGMTString();
            document.cookie = cname + "=" + cvalue + "; " + expires;
            }

            function getCookie(cname)
            {
            var name = cname + "=";
            var ca = document.cookie.split(';');

            for(var i=0; i<ca.length; i++) 
              {
              var c = ca[i].replace(/^\s+|\s+$/g, '');
              if (c.indexOf(name)==0) return c.substring(name.length,c.length);
              }
            return "";
            }

            function checkCookie() {

                // D'abord on ramasse les variables dans les cookies
                var username=getCookie("username");
                var password=getCookie("password");

                // On change les champs du formulaire pour ces variables
                $("#username").val(username);
                $("#password").val(password);            
            }


            $(document).ready(function() {

                checkCookie();      
   
        
            $(".signin").click(function(e) {          
                e.preventDefault();
                $("fieldset#signin_menu").toggle();
                $(".signin").toggleClass("menu-open");
                
                // Ajouté ceci pour cacher messages erreur au départ
                $('#no-username').hide();
                $('#no-password').hide();
                $('#no-nothing').hide();
                $('#no-email').hide();
            });
            
            $("fieldset#signin_menu").mouseup(function() {
                return false
            });

            $(document).mouseup(function(e) {
                if($(e.target).parent("a.signin").length==0) {
                    $(".signin").removeClass("menu-open");
                    $("fieldset#signin_menu").hide();
                }
            });	
            
            // Script pour valider username et password pour Eventum
            $('#login_form').submit(function() {

                // Ajouté ceci pour cacher messages erreur au départ
                $('#no-username').hide();
                $('#no-password').hide();
                $('#no-nothing').hide();
                $('#no-email').hide();

                username = $('#username').val();
                password = $('#password').val();            

                errmessage = '';
                if(username == '') { 
                    //errmessage = 'Veuillez entrer un nom utilisateur'; 
                    errmessage = "#no-username";
                }
                if(password == '') { 
                    //errmessage = 'Veuillez entrer un mot de passe'; 
                    errmessage = "#no-password";
                }
                if(username == '' && password == '') { 
                    //errmessage = 'Veuillez entrer un nom utilisateur et un mot de passe'; 
                    errmessage = "#no-nothing";
                }
                
                // Check if the email address is valid
                var rege = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
                if(rege.test($('#username').val())){  
                } else {
                    //errmessage = 'Le nom utilisateur doit être une adresse courriel complète';
                    errmessage = "#no-email";
                }

                if(errmessage) { 
                    //alert(errmessage);
                    $(errmessage).show();
                    return false;
                }

                    // Si le bouton Remember est coché, on enregistre les variables dans des cookies pour la prochaine fois.
                    if ($('#remember_login').is(':checked')) {
                        setCookie("username",username,365); // 365 = valide pour un an
                        setCookie("password",password,365); // 365 = valide pour un an
                    } else {
                        // Si le bouton Remember est pas coché, on efface les cookies existants.
                        document.cookie = "username=; expires=Thu, 01 Jan 1970 00:00:00 GMT";
                        document.cookie = "password=; expires=Thu, 01 Jan 1970 00:00:00 GMT";                        
                    }

                return true;
            });	
            
            // Fin du script de validation de champs	
            
        });

</script>

