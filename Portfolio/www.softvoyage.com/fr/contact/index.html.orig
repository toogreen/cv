 // Ajouté script qui suit pour faire le toggle des tabs et maps entre Montréal et Toronto
 
  $(document).ready(function(){
      $( "#button-montreal" ).click(function() {

          //Cacher autres onglets
          $("#li-laval").removeClass("active");
          $("#li-toronto").removeClass("active");

          // Afficher onglet Montreal
          $("#li-montreal").addClass("active");

          //Cacher les autres maps
          $("#tab-laval").hide();
          $("#tab-toronto").hide();

          // Afficher map Montreal
          $("#tab-montreal").show();
      });

      $( "#button-toronto" ).click(function() {

          //Cacher autres onglets
          $("#li-laval").removeClass("active");
          $("#li-montreal").removeClass("active");

          // Afficher onglet Toronto
          $("#li-toronto").addClass("active");

          //Cacher les autres maps
          $("#tab-laval").hide();     
          $("#tab-montreal").hide();

          // Aficher Map Toronto
          $("#tab-toronto").show();
      });

      $( "#button-laval" ).click(function() {

          // CACHER les autres onglets
          $("#li-montreal").removeClass("active");
          $("#li-toronto").removeClass("active");

          // Activer l'onglet laval
          $("#li-laval").addClass("active");

          // Cacher les autres Maps
          $("#tab-toronto").hide();     
          $("#tab-montreal").hide();

          // Afficher Map de Laval
          $("#tab-laval").show();
      });
  });
</script>