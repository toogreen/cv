/*Ajout par David pour fonts */
@font-face {
  font-family: "Century Gothic";
  src: url(gothic.ttf);
}

@font-face {
	font-family: "Open Sans";
	src: url(OpenSans-Regular.ttf);
	/*src: url(http://fonts.googleapis.com/css?family=Open+Sans);*/
}

/* Ajout par David pour bouton dans classe box ET p */
.box .button, .texte-normal .button{
	color:#ffffff; 
	font:bold 10px "Century Gothic"; 
	line-height:18px; text-align:center; 
	text-transform:uppercase; 
	background:url(images/button_bg_small.png) no-repeat;
	width: 75px;
	height: 23px;
	float: right;
	bottom: 100px;
	padding-top: 2px;
}

.center_text .button{
	color:#ffffff; 
	font:bold 10px "Century Gothic"; 
	line-height:18px; text-align:center; 
	text-transform:uppercase; 
	background:url(images/button_bg_small.png) no-repeat;
	width: 75px;
	height: 23px;
	float: right;
	bottom: 100px;
	padding-top: 2px;
}

