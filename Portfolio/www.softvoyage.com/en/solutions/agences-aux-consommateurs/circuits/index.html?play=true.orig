// FONCTION AJOUTEE PAR DAVID GAGNON POUR QUERY STRING RAMASSER VARIABLES DANS URL DANS LE HAUT
function querystring(key) {
   var re=new RegExp('(?:\\?|&)'+key+'=(.*?)(?=&|$)','gi');
   var r=[], m;
   while ((m=re.exec(document.location.search)) != null) r.push(m[1]);
   return r;
}

var play = querystring('play');

// SI ON A ENVOYE LA VARIABLE PLAY A TRUE, JOUER LA VIDEO AUTOMATIQUEMENT ETC
if ( play == "true" ) {
  callPlayer("ytplayer", "playVideo");
  $("#frame-image").hide();
  $("#frame-video").show();
}

