        	$(this).find('.thumb')
        		.css({"height":"250px"});

        	// On agrandit le rectangle rouge pour faire place aux sous-sections
        	$(this).find('.left-bg:first-child')
        		.css({"height":"122px"});

        	// On affiche le menu de sous-section correspondant
        	//$(this).find('.menus-sous-sections:first-child').show();
