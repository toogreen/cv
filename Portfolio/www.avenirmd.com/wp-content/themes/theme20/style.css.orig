article {
	margin-top:1px;
	width:100%;
	position:relative;
	clear:both;
	/*background:#f4f7f9;*/
	background:#fafafa;
	border-bottom:4px solid #e5ecf1;
	}
article:first-child {
	margin:0;
}	
	article .inside {
		padding:27px 31px 25px 32px;
	}
	article.maxheight {
		border:none;
		background:url(images/border-bottom.gif) repeat-x left bottom #f4f7f9;
	}

/* Post header */
article header h2 {
	margin-bottom:.5em;
	}
article.single-post {
	margin-bottom:30px;
	overflow:hidden;
	width:100%;
	}

/* Featured thumbnail */
.featured-thumbnail {
	float:left;
	margin:5px 20px 10px 0;
	border:1px solid white;
}
	a.featured-thumbnail:hover {
		border-color:#667984;
	}
	.featured-thumbnail.large {
		display:inline-block;
		float:none;
	}
	

/* Post content */
.post-content {
	padding:0 0 0 34px;	
}
	.post-content.alt {
		padding-right:40px;
	}
	.post-content.nopadding {
		padding:0;
	}
