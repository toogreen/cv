/* General
---------------------------------------- */

@font-face {
	font-family: "Open Sans";
	src: url(OpenSans-Regular.ttf);
	/*src: url(http://fonts.googleapis.com/css?family=Open+Sans);*/
}

html,
body {
	min-width:960px;
	}
body{
	background:url(images/tail-top.gif) repeat-x center top white;
	color: #9bafbb;
	font: 100%/1.125em Arial, Helvetica, sans-serif;
	position:relative;
}

#main {
	font-size:.875em;
}
.container {
	width: 978px;
	margin: 0 auto;
	position:relative;
}


/* List styling */
dl dt {
	background:url(images/list-arrow.gif) no-repeat 0 3px;
	padding:0 0 3px 32px;
	margin-bottom:.2em;
	font-size:18px;
	line-height:1.2em;
	color:#191919;
	}
dl dd {
	padding:0 0 0 32px;
	margin:0 0 1.5em 0;
	}




/* Header
---------------------------------------- */
