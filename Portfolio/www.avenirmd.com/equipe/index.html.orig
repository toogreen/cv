        		//$(this).find('img:first-child').css('opacity', '0');
        		//$(this).find('.attachment-custom_size-gray:first-child').css('opacity', '0');
        		$(this).find('img:first-child').stop().animate({opacity:1}, 1000);

        	// On rapetisse le layer "thumb" qui est derrière la photo pour faire place aux menus de sous-sections
